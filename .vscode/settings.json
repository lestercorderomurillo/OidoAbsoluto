{
    "files.autoSave": "afterDelay",
    "files.associations": {
        "*.pexec": "php",
        "*.phar": "php",
    },
    "intelephense.files.associations": [
        "*.php",
        "*.phps"
    ],
    "debug.allowBreakpointsEverywhere": true,
    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {
            "textMateRules": [
                {
                    "scope": [
                        "source.hphp",
                    ],
                    "settings": {
                        "foreground": "#d4d4d4"
                    }
                },
                {
                    "scope": [
                        "source.hphp.puntuaction",
                        "source.hphp.block.fn",
                        "source.hphp.node.prop.value"
                    ],
                    "settings": {
                        "foreground": "#a0a0a0"
                    }
                },
                {
                    "scope": [
                        "source.hphp.doctype",
                        "source.hphp.keyword",
                    ],
                    "settings": {
                        "foreground": "#7d69f0"
                    }
                },
                {
                    "scope": [
                        "source.hphp.type",
                        "source.hphp.namespace.definition",
                        "source.hphp.use.definition"
                    ],
                    "settings": {
                        "foreground": "#4EC9B0"
                    }
                },
                {
                    "scope": [
                        "source.hphp.fn.name",
                    ],
                    "settings": {
                        "foreground": "#eedaa3"
                    }
                },
                {
                    "scope": [
                        "source.hphp.variable",
                        "source.hphp.node.content",
                        "source.hphp.fn.variable"
                    ],
                    "settings": {
                        "foreground": "#97eaf5"
                    }
                },
                {
                    "scope": [
                        "source.hphp.comments",
                    ],
                    "settings": {
                        "foreground": "#abdd8e"
                    }
                },
                {
                    "scope": [
                        "source.hphp.node._content",
                    ],
                    "settings": {
                        "foreground": "#569CD6"
                    }
                },
                {
                    "scope": [
                        "source.hphp.keyword.node.component",
                    ],
                    "settings": {
                        "foreground": "#f8d86e"
                    }
                }
            ]
        }
    }
}